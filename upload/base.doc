            <ul>
            {% for article in rencent_article_list %}
                <li>
                    <a href="/article/{{ article.id }}/">{{ article.title }} </a>
                </li>
            {% if forloop.last %}

             {% endif %}
            {% empty %}
                No Article
            {% endfor %}
            </ul>
        </div>
    <!--MaxViews-->
        <div id="maxViews" class="widget widget_recent_entries">
            <h3 class="widget-title">阅读排行</h3>
            {% get_maxViews as maxView_list %}
            <ul>
            {% for article in maxView_list %}
                <li>
                    <a href="/article/{{ article.id }}/">{{ article.title }}</a>
                    <small>({{article.views}})</small>
                </li>
            {% empty %}
                No Article
            {% endfor %}
            </ul>
        </div>
<!--Archives-->
        <div id="archives" class="widget widget_archive">
            <h3 class="widget-title">文章归档</h3>
            {% archives as archives_list %}
            <ul>
            {% for archive in archives_list %}
                <li><a href='/archive/{{ archive.year }}/{{ archive.month }}/{{ archive.day }}/'>{{ archive.year }}年 {{ archive.month }}月 {{ archive.day }}日</a></li>
            {% empty %}
                No Archives
            {% endfor %}
            </ul>
        </div>
<!--Categories-->
        <div id="categories" class="widget widget_categories">
            <h3 class="widget-title">文章分类</h3>
            {% get_categories as categories_list %}
            <ul>
            {% for category in categories_list %}
                <li class="cat-item"><a href="/category/{{ category.id }}/">{{ category.name }}</a> <small>({{ category.num_posts }})</small></li>
            {% empty %}
                No category
            {% endfor %}
            </ul>
        </div>		
<!--Tags-->
        <div id="tag_cloud" class="widget widget_tag_cloud">
            <h3 class="widget-title">文章标签</h3>
            {% get_tags as tags_list %}
            <div class="tagcloud">
            {% for tag in tags_list %}
                <a href='/tag/{{ tag}}' class='tag-link'>#{{ tag }}</a>
            {% empty %}
                No Tags
            {% endfor %}
            </div>
        </div>
<!--Recent Comments-->
        <div id="latest_comments" class="widget widget_categories">
            <h3 class="widget-title">最新评论</h3>
            {% get_comments as comments_list %}
            <ul>
            {% for comment in comments_list %}
                <li class="cat-item"><a href="/article/{{ comment.post_id }}/#comments">{{ comment.text }} - <em>{{ comment.name }}</em></a></li>
            {% empty %}
                No Comments
            {% endfor %}

            </ul>
        </div>	

    </div>
